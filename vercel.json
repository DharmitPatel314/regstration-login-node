{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      }
    ],
    "functions": {
      "api/auth/register": {
        "runtime": "@vercel/node",
        "timeout": 30
      }
    }
  }
  
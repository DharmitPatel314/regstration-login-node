{
    "functions": {
      "api": {
        "runtime": "nodejs12.x",
        "timeout": 30
      }
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/api/auth/register",
        "dest": "index.js"
      }
    ]
  }